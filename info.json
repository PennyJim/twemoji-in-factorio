{
  "name": "twemoji-in-factorio",
  "version": "0.3.4",
  "title": "Twemoji",
  "author": "PennyJim",
  "factorio_version": "2.0",
  "dependencies": [
    "base >= 2.0",
    "?better-chat >= 0.1"
  ],
  "description": "This mod adds in Twemoji as virtual signals",
  "package": {
    "scripts": {
      "prepackage": "bash ./data-preprocessing/prepackage.sh"
    },
    "ignore": [
      "emojibase/**",
      "twemoji/**",
      "data-preprocessing/**",
      "zipper.sh"
    ],
    "sync_portal_details": true,
    "no_portal_upload": true,
    "no_git_push": true
  }
}